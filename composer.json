{
    "name": "cretueusebiu/laravel-vue-spa",
    "description": "A Laravel-Vue SPA starter project template.",
    "keywords": [
        "spa",
        "laravel",
        "vue"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.1",
        "bensampo/laravel-enum": "^6.2",
        "fruitcake/laravel-cors": "^3.0",
        "google/apiclient": "2.13",
        "guzzlehttp/guzzle": "^7.2",
        "jbohme/nominatim-laravel": "^1.0",
        "laminas/laminas-code": "4.8",
        "laravel/framework": "^9.0",
        "laravel/socialite": "^5.1",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.2",
        "malhal/laravel-geographical": "^1.0.5",
        "php-open-source-saver/jwt-auth": "^2.0",
        "sendinblue/api-v3-sdk": "7.x.x"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.4.0",
        "deployer/deployer": "^7.1",
        "doctrine/dbal": "^2.9",
        "spatie/laravel-ignition": "^1.0",
        "fzaninotto/faker": "^1.9.1",
        "laravel/dusk": "^6.11",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/dusk"
            ]
        },
        "google/apiclient-services": [
            "Books"
        ]
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "pre-install-cmd": [
          "Composer\\Config::disableProcessTimeout"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php artisan jwt:secret --force --ansi"
        ],
        "post-update-cmd": "Google\\Task\\Composer::cleanup"
    }
}
